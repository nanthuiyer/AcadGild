   "source": [
    "import numpy as np\n",
    "\n",
    "def vander_func(a, n) :\n",
    "    return np.vander(a, n, increasing=False)\n",
    "    \n",
    "a = [2,3,4,5]\n",
    "vander_func(a,5)\n"
